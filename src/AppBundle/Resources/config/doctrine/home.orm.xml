<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\homeRepository" name="AppBundle\Entity\home">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="picture" type="string" column="picture" length="255"/>
    <field name="phonePicture" type="string" column="phone_picture" length="255"/>
    <field name="text" type="string" column="text" length="255"/>
    <field name="altTitle" type="string" column="alt_title" length="255"/>
    <field name="altPicture" type="string" column="alt_picture" length="255"/>
  </entity>
</doctrine-mapping>
